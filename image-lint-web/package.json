{
    "name": "image-lint-web",
    "keywords": [
        "image",
        "lint",
        "compression",
        "web"
    ],
    "author": "Aaron Spaulding <aaron@sachimp.com>",
    "homepage": "https://github.com/AaronAsAChimp/image-lint#readme",
    "license": "MIT",
    "engines": {
        "node": ">=16.0.0"
    },
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "directories": {
        "test": "__tests__"
    },
    "source": [
        "web/index.html",
        "web/404.html"
    ],
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AaronAsAChimp/image-lint.git"
    },
    "scripts": {
        "test": "echo \"Error: run tests from root\" && exit 1",
        "ci:lint": "eslint . --config .eslintrc.json --ext .js,.vue",
        "build": "BASE_PATH=\"/image-lint/\" parcel build --dist-dir ../docs --public-url /image-lint/",
        "serve": "parcel --dist-dir ../docs"
    },
    "bugs": {
        "url": "https://github.com/AaronAsAChimp/image-lint/issues"
    },
    "type": "module",
    "dependencies": {
        "@fontsource/lato": "^4.5.5",
        "buffer": "^6.0.3",
        "crypto-browserify": "^3.12.0",
        "html-loader": "^4.2.0",
        "path-browserify": "^1.0.1",
        "vue": "^3.2.31",
        "vue-github-button": "^3.0.3",
        "vue-multiselect": "^3.0.0-alpha.2",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@parcel/transformer-vue": "^2.12.0",
        "eslint": "^8.24.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-vue": "^9.5.1",
        "parcel": "^2.12.0",
        "parcel-transformer-markdown": "^3.0.1",
        "process": "^0.11.10",
        "rehype-shiki": "^0.0.9",
        "rehype-slug": "^6.0.0",
        "rehype-stringify": "^10.0.0",
        "rehype-toc": "^3.0.2",
        "remark-gfm": "^4.0.0",
        "remark-parse": "^11.0.0",
        "remark-rehype": "^11.0.0",
        "stream-browserify": "^3.0.0",
        "unified": "^11.0.4",
        "vue-template-compiler": "^2.7.10"
    },
    "version": "3.1.0"
}
