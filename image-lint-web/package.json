{
    "name": "image-lint-web",
    "version": "2.1.1",
    "description": "A web application for image-lint",
    "keywords": [
        "image",
        "lint",
        "compression",
        "web"
    ],
    "author": "Aaron Spaulding <aaron@sachimp.com>",
    "homepage": "https://github.com/AaronAsAChimp/image-lint#readme",
    "license": "MIT",
    "bin": {
        "image-lint-server": "bin/image-lint-server.js"
    },
    "directories": {
        "test": "__tests__"
    },
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AaronAsAChimp/image-lint.git"
